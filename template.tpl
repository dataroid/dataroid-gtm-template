    case TAG_ACTIONS.CLEAR_CONTINENT:
      clearContinent();
      break;

    case TAG_ACTIONS.SET_REGION:
      setRegion();
      break;

    case TAG_ACTIONS.CLEAR_REGION:
      clearRegion();
      break;

    case TAG_ACTIONS.SET_COUNTRY:
      setCountry();
      break;

    case TAG_ACTIONS.CLEAR_COUNTRY:
      clearCountry();
      break;

    case TAG_ACTIONS.SET_CITY:
      setCity();
      break;

    case TAG_ACTIONS.CLEAR_CITY:
      clearCity();
      break;

    case TAG_ACTIONS.SET_COORDINATE:
      setCoordinate();
      break;

    case TAG_ACTIONS.CLEAR_COORDINATE:
      clearCoordinate();
      break;

    case TAG_ACTIONS.SET_REQUEST_OPTIONS:
      setRequestOptions();
      break;
  }

  data.gtmOnSuccess();
};

const onInjectScriptFailure = () => {
  logError('Failed to load Dataroid Web SDK');

  data.gtmOnFailure();
};

