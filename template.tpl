const logToConsole = require('logToConsole');
const injectScript = require('injectScript');
const getReferrerUrl = require('getReferrerUrl');
const callInWindow = require('callInWindow');
const templateStorage = require('templateStorage');
const makeNumber = require('makeNumber');

// Constants
const DEFAULT_SDK_URL = 'https://cdn.dataroid.com/sdk/web/dataroid-websdk.js';
const DEFAULT_SDK_ENDPOINT = 'https://api.dataroid.com';
const LOG_PREFIX = '[Dataroid GTM]';
const TEMPLATE_STORAGE_KEYS = {
  IS_INITIALIZED: 'IS_INITIALIZED',
  SDK_URL: 'SDK_URL',
  PRINT_LOGS: 'PRINT_LOGS',
};
const TAG_ACTIONS = {
  INIT: 'init',
  CUSTOM_EVENT: 'customEvent',
  SET_USER_PROFILE: 'setUserProfile',
  CLEAR_USER_PROFILE: 'clearUserProfile',
  ENABLE_PUSH: 'enablePush',
  DISABLE_PUSH: 'disablePush',
  SET_LANGUAGE: 'setLanguage',
  PAGE_VIEW: 'pageView',
  SET_SUPER_ATTRIBUTE: 'setSuperAttribute',
  CLEAR_SUPER_ATTRIBUTE: 'clearSuperAttribute',
  CLEAR_ALL_SUPER_ATTRIBUTES: 'clearAllSuperAttributes',
  SET_CONTINENT: 'setContinent',
  CLEAR_CONTINENT: 'clearContinent',
  SET_REGION: 'setRegion',
  CLEAR_REGION: 'clearRegion',
  SET_COUNTRY: 'setCountry',
  CLEAR_COUNTRY: 'clearCountry',
  SET_CITY: 'setCity',
  CLEAR_CITY: 'clearCity',
  SET_COORDINATE: 'setCoordinate',
  CLEAR_COORDINATE: 'clearCoordinate',
  SET_REQUEST_OPTIONS: 'setRequestOptions',
};

// Global variables
const sdkUrl =
  data.sdkUrl ||
  templateStorage.getItem(TEMPLATE_STORAGE_KEYS.SDK_URL) ||
  DEFAULT_SDK_URL;
const sdkEndpoint = data.sdkEndpoint || DEFAULT_SDK_ENDPOINT;
const printLogs =
  data.printLogs || templateStorage.getItem(TEMPLATE_STORAGE_KEYS.PRINT_LOGS);

// Utils
const logInfo = function () {
  if (printLogs) {
    logToConsole(LOG_PREFIX, 'Info:', arguments);
  }
};

const logError = function () {
  if (printLogs) {
    logToConsole(LOG_PREFIX, 'Error:', arguments);
  }
};

const normalize = (stringValue) => {
  let normalizedValue = makeNumber(stringValue) || stringValue;

  if (stringValue === 'null') {
    normalizedValue = null;
  } else if (stringValue === 'undefined') {
    normalizedValue = undefined;
  } else if (stringValue === 'true') {
    normalizedValue = true;
  } else if (stringValue === 'false') {
    normalizedValue = false;
  }

  return normalizedValue;
};

const commaSeparatedStringToArray = (stringArray) =>
  stringArray.split(',').map((stringValue) => normalize(stringValue.trim()));

const checkIsInitialized = () => {
  if (!templateStorage.getItem(TEMPLATE_STORAGE_KEYS.IS_INITIALIZED)) {
    logError('Dataroid is not initialized');

    return false;
  }

  return true;
};

// Tag actions
const init = () => {
  if (!templateStorage.getItem(TEMPLATE_STORAGE_KEYS.IS_INITIALIZED)) {
    const extraConfig = {
      Session: {
        inApp: {
          enabled: data.inAppEnabled,
        },
      },
      Page: {
        enabled: data.pageEnabled,
        ignoreQueryParams: data.ignoreQueryParams,
        screenCapture: {
          enabled: data.screenCaptureEnabled,
        },
      },
    };

    if (data.pushEnabled) {
      extraConfig.Push = {
        enabled: true,
        firebaseOptions: {
          apiKey: data.firebaseApiKey,
          projectId: data.firebaseProjectId,
          messagingSenderId: data.firebaseMessagingSenderId,
          appId: data.firebaseAppId,
        },
        vapidKey: data.firebaseVapidKey,
        swPath: data.swPath,
        swScope: data.swScope || false,
      };
    }

    if (data.sessionTimeout) {
      extraConfig.Session.timeout = makeNumber(data.sessionTimeout);
    }

    if (data.inAppCloseDelay) {
      extraConfig.Session.inApp.closeDelay = makeNumber(data.inAppCloseDelay);
    }

    if (data.pageCheckInterval) {
      extraConfig.Page.checkInterval = makeNumber(data.pageCheckInterval);
    }

    if (data.screenCaptureDelay) {
      extraConfig.Page.screenCapture.delay = makeNumber(
        data.screenCaptureDelay
      );
    }

    if (data.eventStorageLimit) {
      extraConfig.Event = {
        storageLimit: makeNumber(data.eventStorageLimit),
      };
    }

    logInfo('Initializing', extraConfig);

    callInWindow(
      'dataroid.init',
      data.sdkKey,
      sdkEndpoint,
      data.appVersion,
      data.appName,
      extraConfig
    );

    templateStorage.setItem(TEMPLATE_STORAGE_KEYS.IS_INITIALIZED, true);
    templateStorage.setItem(TEMPLATE_STORAGE_KEYS.SDK_URL, data.sdkUrl);
    templateStorage.setItem(TEMPLATE_STORAGE_KEYS.PRINT_LOGS, data.printLogs);
  }
};

const sendCustomEvent = () => {
  const extras = {};

  if (data.customEventParams) {
    data.customEventParams.forEach((paramData) => {
      extras[paramData.key] = paramData.isArrayValue ?
        commaSeparatedStringToArray(paramData.value) :
        normalize(paramData.value);
    });
  }

  logInfo('Sending a custom event', data.customEventName, extras);

  callInWindow('dataroid.track', data.customEventName, extras);
};

const setUserProfile = () => {
  const user = {
    cid: data.userProfileCID,
    email: data.userProfileEmail,
    phone: data.userProfilePhone,
    nid: data.userProfileNID,
    fn: data.userProfileFN,
    ln: data.userProfileLN,
    gn: data.userProfileGN,
    dob: data.userProfileDOB,
  };

  if (data.userProfileExtraParams) {
    user.data = data.userProfileExtraParams.map((paramData) => ({
      key: paramData.key,
      value: normalize(paramData.value),
    }));
